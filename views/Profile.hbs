<div class="container-fluid">
    <div class="profile-header">
        <div class="profile-img">
            {{#if user.avatarUrl}}
            <img alt="Avatar-{{user.name}}" src="/{{user.avatarUrl}}" width="100%" max-height="100px" min-height="100px"
                alt="Profile Image">
            {{else}}
            <img src="data:image/png;base64,"
                width="100%" max-height="100px" min-height="100px" alt="Profile Image">
            {{/if}}
        </div>
        <div class="profile-nav-info">
            <h3 class="user-name">{{user.name}}</h3>
            {{#if user.lastname}}
            <h3 class="user-name">{{user.lastname}}</h3>
            {{else}}

            {{/if}}
            <div class="address">
                {{#if user.city}}
                <p id="state" class="state">{{user.city}},</p>
                {{else}}
                <p id="state" class="state">User adres not found</p>
                {{/if}}
                {{#if user.state}}
                <span id="country" class="country">{{user.state}}</span>
                {{else}}
                <span id="country" class="country">.</span>
                {{/if}}
            </div>

        </div>
        <div class="profile-option">
            <div class="notification">
                <i class="fa fa-bell"></i>
                <span class="alert-message">0</span>
            </div>
        </div>
    </div>

    <div class="main-bd">
        <div class="left-side">
            <div class="profile-side">
                {{#if user.Telnumber}}
                <p class="mobile-no"><i class="fa fa-phone"></i>+{{user.Telnumber}}</p>
                {{else}}
                <p class="mobile-no"><i class="fa fa-phone"></i>Addto Your Number</p>
                {{/if}}
                <p class="user-mail"><i class="fa fa-envelope"></i>{{user.email}}</p>
                <div class="user-bio" style="overflow-wrap: break-word;">
                    <h3>Bio</h3>
                    {{#if user.bio}}
                    <p class="bio">{{user.bio}}</p>
                    {{else}}
                    <p class="bio"></p>
                    {{/if}}
                </div>
            </div>

        </div>
        <div class="right-side">

            <div class="nav">
                <ul>
                    <li onclick="tabs(2)" class="user-setting"> Settings</li>
                </ul>
            </div>
            <div class="profile-body">
                <div class="profile-settings tab">
                    <div class="account-setting">
                        <form action="/Profile" method="POST" enctype="multipart/form-data">
                            <div class="row">
                                <div class="col">
                                    <input type="text" name="name" class="form-control" placeholder="First name"
                                        required>
                                </div>
                                <div class="col">
                                    <input type="text" name="lastname" class="form-control" placeholder="Last name"
                                        required>
                                </div>
                            </div>
                            <div class="form-row" style="place-items: center;">
                                <div class="col-7">
                                    <input type="text" name="city" class="form-control" placeholder="City" required>
                                </div>
                                <div class="col">
                                    <input type="text" name="state" class="form-control" placeholder="State" required>
                                </div>
                                <div class="col">
                                    <input type="number" name="zip" class="form-control" placeholder="Zip" required>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col">
                                    <input type="number" name="Telnumber" class="form-control"
                                        placeholder="Enter your telephone number" required>
                                </div>
                                <div class="col">
                                    <input type="file" placeholder="Your Photo" name="avatar" class="form-control"
                                        required>
                                </div>
                            </div>
                            <div class="form-floating">
                                <textarea type="text" name="bio" placeholder="Your Biografia" class="form-control"
                                    required value="{{data.descr}}"></textarea>
                            </div>
                            <button type="submit" class="mt-3 btn btn-primary">Change</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>